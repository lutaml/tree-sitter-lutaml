{
  "name": "tree-sitter-lutaml",
  "version": "0.1.0",
  "description": "LutaML grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "init": "tree-sitter init",
    "generate": "tree-sitter generate",
    "install": "node-gyp-build",
    "lint": "eslint grammar.js",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test:custom": "tree-sitter test && script/parse-examples",
    "test": "node --test bindings/node/*_test.js"
  },
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lutaml/tree-sitter-lutaml.git"
  },
  "keywords": [
    "incremental",
    "tree-sitter",
    "parsing",
    "lutaml"
  ],
  "author": "Ribose Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lutaml/tree-sitter-lutaml/issues"
  },
  "homepage": "https://github.com/lutaml/tree-sitter-lutaml#readme",
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  }
}
